		methods: {
			//分配
			allot(name) {
				this.startDate = '';
				this.endDate = '';
				this.choosetime = '';
				this.demand = '';
				this.uid = uni.getStorageSync("USER_ID")
				let param = {
					uid: this.uid,
					task_id: '',
					unit_id: ''
				}
				helper.request(helper.websiteUrl + 'v1/queryUserBySpecialty', param, 'POST', true, res => {
					let userId = res.group
					userId.forEach(item => {
						this.personSheetList.push({
							id: item.USER_ID,
							text: item.NAME
						})
					})
				})
				this.showAllot = true;
				helper.request(helper.websiteUrl + 'v1/questionsList', param, 'POST', true, res => {
					this.allotName = name;
					this.showAllot = true;
				})
			},
			// 点击collapsehead头部
			headClick(index) {
				this.clickindex = index;
				this.collapseList[index].isShow = !this.collapseList[index].isShow;
				this.collapseList.map((e, index) => {
					if (index != this.clickindex) {
						e.isShow = false;
					}
				})
			},
			changetime(e) {
				this.startDate = e.startDate;
				this.endDate = e.endDate;
				this.choosetime = e.startDate + '-' + e.endDate;
			},
			openPerson(index) {
				this.openindex = index;
				this.showPersonList = true;
			},
			personSheetCallback(index) {
				this.collapseList[this.openindex].person = this.personSheetList[index].text;
			},
			openchildPerson(index, index1) {
				this.openindex = index;
				this.openchildindex = index1;
				this.showPersonchildList = true;
			},
			personchildSheetCallback(index) {
				this.collapseList[this.openindex].detailList[this.openchildindex].person = this.personSheetList[index].text;
			},
			close() {
				this.showAllot = false;
			},
			cancel() {
				this.showAllot = false;
			},
			sure() {
				if (!this.choosetime) {
					this.$refs.uToast.show({
						title: '请选择整改期限',
						type: 'warning'
					})
					return;
				}
				if (!this.demand) {
					this.$refs.uToast.show({
						title: '请输入整改要求',
						type: 'warning'
					})
					return;
				}
				if (this.demand.length > 200) {
					this.$refs.uToast.show({
						title: '整改要求范围为1-200字',
						type: 'warning'
					})
					return;
				}
				this.showAllot = false;
			},
			see(id, unit_id, name) {
				let param = {
					id: id,
					name: name,
					unit_id: unit_id
				}
				uni.navigateTo({
					url: '../../pages/problem/list?param=' + JSON.stringify(param)
				})
			},
			jump(list) {
				uni.navigateTo({
					url: '../../pages/process/process?list=' + JSON.stringify(list)
				})
			},

			//通过
			pass(id, index) {

				helper.request(helper.websiteUrl + 'v1/pass', {
					uid: '',
					id: id
				}, 'POST', true, res => {


					this.list.splice(index, 1);
				})
			},
			//驳回
			refuse(id, index) {
				this.show = true;
				this.id = id
				this.rejectindex = index
			},
			//确认驳回
			confirm() {
				if (!this.reason) {
					uni.showToast({
						title: '驳回理由不能为空',
						icon: 'none'
					})
					this.$refs.uModal.clearLoading();
					this.reason = null;
					return;
				} else {
					let uid = uni.getStorageSync('USER_ID')
					let param = {
						uid: uid,
						content: this.reason,
						id: this.id
					}
					helper.request(helper.websiteUrl + 'v1/reject', param, 'POST', true, res => {
						this.list.splice(this.rejectindex, 1);
					})
					this.reason;
					this.show = false;
				}
			},
			//待处理数据拉去
			queryUserPending() {
				this.doneLoading = true;
				this.status = 'loading';
				helper.request(helper.websiteUrl + 'v1/queryUserPending', {
					uid: '',
					page: this.pageNum,
					size: this.pageSize
				}, 'POST', true, res => {
					if (res&&res.length) {
						this.pages = parseInt(res[0].total)
						// this.pageNum = parseInt(res[0].currentPage)
						const recordsList = res; //模拟请求到的数据
						this.list = this.list.concat(recordsList);
						this.changing = false;
						this.pageNum++;
						this.doneLoading = false;
						if (this.pageNum > this.pages) {
							this.status = 'nomore';
						}
					} else {
						this.changing = false;
						this.list = [];
					}
				})
			},
			// 上拉加载
			onreachBottom(e) {
				if (this.pageNum > this.pages) {
					this.status = 'nomore';
					return
				}
				if (this.curNow == 0 && this.doneLoading == false) {
					this.queryUserPending();
				} else if (this.curNow == 1 && this.doneLoading == false) {
					this.queryUserProcessed();
				}
			},

			//下拉刷新
			loadmore() {
				if (this.pageNum > this.pages) {
					this.status = 'nomore';
					return
				}
				if (this.curNow == 0 && this.doneLoading == false) {
					this.queryUserPending();
				} else if (this.curNow == 1 && this.doneLoading == false) {
					this.queryUserProcessed();
				}
			},
			//已处理处理数据拉去
			queryUserProcessed() {
				this.status = 'loading';
				helper.request(helper.websiteUrl + 'v1/queryUserProcessed', {
					uid: '',
					page: this.pageNum,
					size: this.pageSize
				}, 'POST', true, res => {
					if (res&&res.length) {
						this.pages = parseInt(res[0].total)
						// this.pageNum = parseInt(res[0].currentPage)
						const recordsList = res;
						this.list = this.list.concat(recordsList);
						this.changing = false
						this.pageNum++;
						if (this.pageNum > this.pages) {
							this.status = 'nomore';
						}
					} else {
						this.list = []
						this.changing = false
					}

				})
			},
			sectionChange(curNow) {
				this.pageNum = 1;
				this.pages = 1;
				this.pageSize = 10;
				this.doneLoading = false;
				this.changing = true;
				this.list = [];
				if (curNow == 0) {
					this.curNow = curNow
					this.queryUserPending();
				}
				if (curNow == 1) {
					this.curNow = curNow;
					this.queryUserProcessed();
				}
			}
		}
	}
</script>

